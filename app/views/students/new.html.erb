<div>
    <%= form_with(model: @student) do |form| %> 
        <%= form.label :email %> 
        <%= form.text_field :email%> 
        <%= form.label :username %> 
        <%= form.text_field :username %> 
        <%= form.label :password %>
        <%= form.text_field :password %> 
        <%= form.submit %> 
    <% end %> 
</div>